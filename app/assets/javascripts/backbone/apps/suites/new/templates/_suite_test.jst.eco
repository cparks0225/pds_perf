<div class="col-md-12">
  <div class="panel panel-default">
    <div class="panel-body">
      <div class="col-md-8"><%= @.name %></div>
      <div class="col-md-1"><span class="badge">Queries: <%- @queries.length %></span></div>
      <div class="col-md-1">
        <button type="button" class="btn btn-xs btn-danger">Remove</button>
      </div>
      <div class="col-md-1">
        <input type="number" class="form-control" name="iterations-<%= @id %>" value=1> 
      </div>
      <div class="col-md-1">
        <input type="number" class="form-control" name="interval-<%= @id %>" value=0> 
      </div>
    </div>
  </div>
</div>