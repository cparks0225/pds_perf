<div class="row">
  <div class="col-md-1">
    <td><%= @method %></td>
  </div>
  <div class="col-md-4">
    <td><%= @url %></td>
  </div>
  <div class="col-md-6">
    <td> <%- @stringifyQueryData(@data) %> </td>
  </div>
  <div class="col-md-1">
  <% if @can_delete: %>
    <button type="button" class="btn btn-xs btn-danger pull-right">Delete</button>
  <% else: %>
    <button type="button" class="btn btn-xs btn-danger pull-right" disabled="disabled">Delete</button>
  <% end %>
  </div>
</div>
