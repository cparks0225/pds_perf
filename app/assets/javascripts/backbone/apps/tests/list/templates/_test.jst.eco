<div class="panel panel-default">
  <div class="panel-heading">
    <h4 class="panel-title">
      <a data-toggle="collapse" href="#collapseId-<%- @name %>">
        <button class="btn btn-xs btn-danger pull-right">Delete</button>
        <span class="badge pull-right">Queries: <%- @queries.length %></span>
        <%- @name %>
      </a>
    </h4>
  </div>
  <div class="panel-collapse collapse" id="collapseId-<%- @name %>">
    <table class="table">
      <tr>
        <th>Method</th>
        <th>Url</th>
        <th>Data</th>
        <th>Iterations</th>
        <th>Interval</th>
      </tr>
      <% for val, idx in @queries: %>
        <tr>
          <td><%- val.method %></td>
          <td><%- val.url %></td>
          <td><%- @stringifyQueryData(val.data) %></td>
          <td><%- val.iterations %></td>
          <td><%- val.interval %></td>
        </tr>
      <% end %>
    </table>
    
  </div>
</div>